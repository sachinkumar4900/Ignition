{
  "resource-type": "text-resource",
  "content-type": "text/html",
  "text": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Ignition Voice Dashboard</title>\r\n    <style>\r\n        * {\r\n            margin: 0;\r\n            padding: 0;\r\n            box-sizing: border-box;\r\n        }\r\n\r\n        body {\r\n            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\r\n            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n            min-height: 100vh;\r\n            padding: 20px;\r\n        }\r\n\r\n        .container {\r\n            max-width: 1400px;\r\n            margin: 0 auto;\r\n        }\r\n\r\n        header {\r\n            background: white;\r\n            padding: 20px 30px;\r\n            border-radius: 15px;\r\n            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n            margin-bottom: 20px;\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            flex-wrap: wrap;\r\n        }\r\n\r\n        h1 {\r\n            color: #333;\r\n            font-size: 2rem;\r\n        }\r\n\r\n        .voice-controls {\r\n            display: flex;\r\n            gap: 10px;\r\n            align-items: center;\r\n        }\r\n\r\n        .voice-btn {\r\n            padding: 12px 24px;\r\n            border: none;\r\n            border-radius: 8px;\r\n            font-size: 16px;\r\n            font-weight: 600;\r\n            cursor: pointer;\r\n            transition: all 0.3s ease;\r\n        }\r\n\r\n        #startVoice {\r\n            background: #10b981;\r\n            color: white;\r\n        }\r\n\r\n        #startVoice:hover:not(:disabled) {\r\n            background: #059669;\r\n            transform: translateY(-2px);\r\n        }\r\n\r\n        #stopVoice {\r\n            background: #ef4444;\r\n            color: white;\r\n        }\r\n\r\n        #stopVoice:hover:not(:disabled) {\r\n            background: #dc2626;\r\n            transform: translateY(-2px);\r\n        }\r\n\r\n        .voice-btn:disabled {\r\n            opacity: 0.5;\r\n            cursor: not-allowed;\r\n        }\r\n\r\n        .status {\r\n            padding: 8px 16px;\r\n            background: #f3f4f6;\r\n            border-radius: 20px;\r\n            font-weight: 600;\r\n            color: #6b7280;\r\n        }\r\n\r\n        .status.listening {\r\n            background: #dcfce7;\r\n            color: #16a34a;\r\n            animation: pulse 1.5s infinite;\r\n        }\r\n\r\n        @keyframes pulse {\r\n            0%, 100% { opacity: 1; }\r\n            50% { opacity: 0.6; }\r\n        }\r\n\r\n        .transcript-section {\r\n            background: white;\r\n            padding: 20px 30px;\r\n            border-radius: 15px;\r\n            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n            margin-bottom: 20px;\r\n        }\r\n\r\n        .transcript-section h3 {\r\n            color: #333;\r\n            margin-bottom: 10px;\r\n        }\r\n\r\n        .transcript {\r\n            padding: 15px;\r\n            background: #f9fafb;\r\n            border-radius: 8px;\r\n            min-height: 50px;\r\n            font-size: 18px;\r\n            color: #374151;\r\n            border-left: 4px solid #667eea;\r\n        }\r\n\r\n        .feedback {\r\n            margin-top: 10px;\r\n            padding: 10px;\r\n            border-radius: 8px;\r\n            font-weight: 600;\r\n            display: none;\r\n        }\r\n\r\n        .feedback.success {\r\n            background: #dcfce7;\r\n            color: #16a34a;\r\n            display: block;\r\n        }\r\n\r\n        .feedback.error {\r\n            background: #fee2e2;\r\n            color: #dc2626;\r\n            display: block;\r\n        }\r\n\r\n        .dashboard-grid {\r\n            display: grid;\r\n            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\r\n            gap: 20px;\r\n            margin-bottom: 20px;\r\n        }\r\n\r\n        .widget {\r\n            background: white;\r\n            padding: 25px;\r\n            border-radius: 15px;\r\n            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n            transition: transform 0.3s ease;\r\n        }\r\n\r\n        .widget:hover {\r\n            transform: translateY(-5px);\r\n        }\r\n\r\n        .widget h2 {\r\n            color: #333;\r\n            margin-bottom: 20px;\r\n            font-size: 1.4rem;\r\n            border-bottom: 2px solid #667eea;\r\n            padding-bottom: 10px;\r\n        }\r\n\r\n        .widget-content {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 15px;\r\n        }\r\n\r\n        .status-item, .metric-item {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            padding: 10px;\r\n            background: #f9fafb;\r\n            border-radius: 8px;\r\n        }\r\n\r\n        .label {\r\n            font-weight: 600;\r\n            color: #6b7280;\r\n        }\r\n\r\n        .value {\r\n            font-size: 1.2rem;\r\n            font-weight: 700;\r\n            color: #333;\r\n        }\r\n\r\n        .value.on {\r\n            color: #10b981;\r\n        }\r\n\r\n        .value.off {\r\n            color: #ef4444;\r\n        }\r\n\r\n        .alert-list {\r\n            list-style: none;\r\n            max-height: 200px;\r\n            overflow-y: auto;\r\n        }\r\n\r\n        .alert-list li {\r\n            padding: 10px;\r\n            margin-bottom: 8px;\r\n            border-radius: 6px;\r\n            border-left: 4px solid;\r\n        }\r\n\r\n        .alert-info {\r\n            background: #dbeafe;\r\n            border-color: #3b82f6;\r\n            color: #1e40af;\r\n        }\r\n\r\n        .alert-warning {\r\n            background: #fef3c7;\r\n            border-color: #f59e0b;\r\n            color: #92400e;\r\n        }\r\n\r\n        .alert-danger {\r\n            background: #fee2e2;\r\n            border-color: #ef4444;\r\n            color: #991b1b;\r\n        }\r\n\r\n        .controls-grid {\r\n            display: grid;\r\n            grid-template-columns: repeat(2, 1fr);\r\n            gap: 10px;\r\n        }\r\n\r\n        .control-btn {\r\n            padding: 12px;\r\n            border: none;\r\n            border-radius: 8px;\r\n            background: #667eea;\r\n            color: white;\r\n            font-weight: 600;\r\n            cursor: pointer;\r\n            transition: all 0.3s ease;\r\n        }\r\n\r\n        .control-btn:hover {\r\n            background: #5568d3;\r\n            transform: scale(1.05);\r\n        }\r\n\r\n        .help-section {\r\n            background: white;\r\n            padding: 20px 30px;\r\n            border-radius: 15px;\r\n            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n        }\r\n\r\n        .help-section h3 {\r\n            color: #333;\r\n            margin-bottom: 15px;\r\n        }\r\n\r\n        .commands-grid {\r\n            display: grid;\r\n            grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\r\n            gap: 10px;\r\n        }\r\n\r\n        .command-item {\r\n            padding: 10px;\r\n            background: #f9fafb;\r\n            border-radius: 6px;\r\n            border-left: 3px solid #667eea;\r\n        }\r\n\r\n        .command-item strong {\r\n            color: #667eea;\r\n        }\r\n\r\n        @media (max-width: 768px) {\r\n            header {\r\n                flex-direction: column;\r\n                gap: 15px;\r\n            }\r\n            \r\n            .dashboard-grid {\r\n                grid-template-columns: 1fr;\r\n            }\r\n            \r\n            .controls-grid {\r\n                grid-template-columns: 1fr;\r\n            }\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <div class=\"container\">\r\n        <header>\r\n            <h1>üéØ Ignition Dashboard</h1>\r\n            <div class=\"voice-controls\">\r\n                <button id=\"startVoice\" class=\"voice-btn\">\r\n                    üé§ Start Voice Input\r\n                </button>\r\n                <button id=\"stopVoice\" class=\"voice-btn\" disabled>\r\n                    ‚èπÔ∏è Stop Voice Input\r\n                </button>\r\n                <span id=\"voiceStatus\" class=\"status\">Ready</span>\r\n            </div>\r\n        </header>\r\n\r\n        <div class=\"transcript-section\">\r\n            <h3>Voice Command</h3>\r\n            <div id=\"transcript\" class=\"transcript\">Say a command...</div>\r\n            <div id=\"feedback\" class=\"feedback\"></div>\r\n        </div>\r\n\r\n        <div class=\"dashboard-grid\">\r\n            <!-- System Status Widget -->\r\n            <div class=\"widget\" id=\"systemStatus\">\r\n                <h2>System Status</h2>\r\n                <div class=\"widget-content\">\r\n                    <div class=\"status-item\">\r\n                        <span class=\"label\">Engine:</span>\r\n                        <span id=\"engineStatus\" class=\"value off\">OFF</span>\r\n                    </div>\r\n                    <div class=\"status-item\">\r\n                        <span class=\"label\">Temperature:</span>\r\n                        <span id=\"temperature\" class=\"value\">22¬∞C</span>\r\n                    </div>\r\n                    <div class=\"status-item\">\r\n                        <span class=\"label\">Pressure:</span>\r\n                        <span id=\"pressure\" class=\"value\">101 kPa</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <!-- Metrics Widget -->\r\n            <div class=\"widget\" id=\"metrics\">\r\n                <h2>Performance Metrics</h2>\r\n                <div class=\"widget-content\">\r\n                    <div class=\"metric-item\">\r\n                        <span class=\"label\">Speed:</span>\r\n                        <span id=\"speed\" class=\"value\">0 km/h</span>\r\n                    </div>\r\n                    <div class=\"metric-item\">\r\n                        <span class=\"label\">Power:</span>\r\n                        <span id=\"power\" class=\"value\">0 kW</span>\r\n                    </div>\r\n                    <div class=\"metric-item\">\r\n                        <span class=\"label\">Efficiency:</span>\r\n                        <span id=\"efficiency\" class=\"value\">85%</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <!-- Alerts Widget -->\r\n            <div class=\"widget\" id=\"alerts\">\r\n                <h2>Alerts & Notifications</h2>\r\n                <div class=\"widget-content\">\r\n                    <ul id=\"alertList\" class=\"alert-list\">\r\n                        <li class=\"alert-info\">System ready</li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n\r\n            <!-- Controls Widget -->\r\n            <div class=\"widget\" id=\"controls\">\r\n                <h2>Quick Controls</h2>\r\n                <div class=\"widget-content controls-grid\">\r\n                    <button class=\"control-btn\" onclick=\"toggleEngine()\">Toggle Engine</button>\r\n                    <button class=\"control-btn\" onclick=\"resetSystem()\">Reset System</button>\r\n                    <button class=\"control-btn\" onclick=\"increasePower()\">Increase Power</button>\r\n                    <button class=\"control-btn\" onclick=\"decreasePower()\">Decrease Power</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"help-section\">\r\n            <h3>Voice Commands</h3>\r\n            <div class=\"commands-grid\">\r\n                <div class=\"command-item\">\r\n                    <strong>\"start engine\"</strong> - Start the engine\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"stop engine\"</strong> - Stop the engine\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"increase power\"</strong> - Increase power output\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"decrease power\"</strong> - Decrease power output\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"reset system\"</strong> - Reset all systems\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"show temperature\"</strong> - Display temperature\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"show pressure\"</strong> - Display pressure\r\n                </div>\r\n                <div class=\"command-item\">\r\n                    <strong>\"show status\"</strong> - Show system status\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <script src=\"ignition-api.js\"></script>\r\n    <script src=\"script.js\"></script>\r\n</body>\r\n</html>\r\n"
}